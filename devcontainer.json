{
	"name": "Ubuntu",
	"build": {
		"dockerfile": "./Dockerfile"
	},
	"workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind", 
	"workspaceFolder": "/workspace",
	"forwardPorts": [
		8080
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.cpptools-themes",
				"ms-vscode.cpptools",
				"ms-vscode.cpptools-extension-pack"
			]
		}
	}
}
